Configs: {
	MusicConfig: {
		DisplayName: BGM設定
		MusicTogglesHeader: BGM切り替え
		ConditionChangesHeader: コンディション変更

		WorkshopRange: {
			Label: "[i:WorkBench] ワークショップの範囲"
			Tooltip: ワークショップのBGMが再生される範囲を10ブロックから50ブロックの間まで変更します
		}
				
		WorkshopThemes: {
			Label: "[i:UnCalamityModMusic/WorkshopTier1MusicBox] ワークショップのBGM"
			Tooltip:
				'''
				特定の作業台の近くで再生されるワークショップのBGMを切り替えます
				街のBGMを無効にしたい場合は、別のNo Town Music Modを使うか、影の蝋燭を使ってください
				'''
		}

		HardmodeInterlude: {
			Label: "[i:UnCalamityModMusic/HardmodeInterludeMusicBox] ハードモード突入時のBGM"
			Tooltip:
				'''
				ハードモードに突入した時に流れるBGMを切り替えます
				クレジットを無効にしたい場合は別のNo In-game Credits Modを使用してください
				'''
		}
		
		OverrideOtherworldlyMusic: {
			Label: "[i:MusicBoxOWRain] アザーワールドのBGMを上書きする"
			Tooltip:
				'''
				アザーワールドのBGMをVCMMのBGMで上書きするか切り替えます
				この設定はパーティガールのBGM切り替えがオンになっている際に効果があります
				'''
		}

		OverrideAnahitasLureMusic: {
			Label: "[i:UnCalamityModMusic/OceanDayMusicBox] アナヒタの歌を上書きする"
			Tooltip: アナヒタの歌声が海のBGMを上書きするかどうかを切り替えます

		}

		PrioritizeMusicFromOtherMods: {
			Label: "[i:WorldGlobe] 他のModのBGMを優先する"
			Tooltip:
				'''
				他のバニラのBGMを変更するModのBGMをVCMMのBGMより優先するかどうかを切り替えます
				さらなる音楽のカスタマイズについては、他のMod設定で確認してください
				'''
		}
	}

	OtherConfig: {
		DisplayName: その他の設定
		AmbienceModeHeader: 環境音
		OtherChangesHeader: その他の変更

		AmbienceFrequency: {
			Label: "[i:Bunny] 環境音の頻度"
			Tooltip:
				'''
				環境音が通常のBGMに置き換わる頻度を0%から100%で調整します
				100%の場合、ボスおよびイベントを除く全てのBGMが無効になります
				'''
		}

		DisableBeeHiveAmbience: {
			Label: "[i:BeeWax] 蜂の巣の環境音を無効化"
			Tooltip: 蜂の巣の環境音を切り替えます
		}

		DisableSpiderCaveAmbience: {
			Label: "[i:SpiderFang] クモの洞窟の環境音を無効化"
			Tooltip: クモの洞窟の環境音を切り替えます
		}

		MusicBoxResprite: {
			Label: "[i:MusicBox] 未録音のオルゴールの見た目を切り替える"
			Tooltip: 録音されていないオルゴールの見た目を変更します
		}
	}
}

