			Label: 位置オプション
			Tooltip: ""
		}

		VerticalOffset: {
			Label: 垂直オフセット
			Tooltip:
				'''
				住宅パネルの配置オフセット。例えば、バニラではミニマップが開いている場合256を使用
				値が小さいほどパネルは画面の下に近づきます
				'''
		}
	}

	PanelDisplayOptions: {
		Tooltip:
			'''
			有効/無効：住宅パネルを常に表示/表示しない
			記憶：パネルの最後の状態を記憶し、その状態で再開します
			バニラ：インベントリでの状態と同じ状態で住宅パネルを開く
			スマート：記憶と同じだが、インベントリで開いていた場合自動で有効化されます [デフォルト]
			'''

		Enabled: {
			Label: 有効
		}

		Disabled: {
			Label: 無効
		}

		Remember: {
			Label: 記憶
		}

		Vanilla: {
			Label: バニラ
		}

		Smart: {
			Label: スマート
		}
	}

	PanelPositionOptions: {
		Tooltip:
			'''
			バニラ：住宅パネルはインベントリで開いたときと同じ位置に表示します
			ミニマップ開：住宅パネルはミニマップが開いているときと同じ位置に表示します
			ミニマップ閉：住宅パネルはミニマップが閉じているときと同じ位置に表示します
			カスタムオフセット：住宅パネルは以下の垂直オフセットに基づいて表示します
			'''

		Vanilla: {
			Label: バニラ
		}

		MiniMapOpen: {
			Label: ミニマップ開
		}

		MiniMapClosed: {
			Label: ミニマップ閉
		}

		CustomOffset: {
			Label: カスタムオフセット
		}
	}

	MiniMapConfig: {
		Tooltip: ""

		MiniMapBannersOptions: {
