		Tooltip: ""

		OverlayMapBannersOptions: {
			Label: オーバーレイマップバナーオプション
			Tooltip: ""
		}
	}

	ServerConfig: {
		DisplayName: サーバー設定

		LoadQueriedChunks: {
			Label: クエリされたチャンクをロード
			Tooltip:
				'''
				マップタイルがクエリされたときに、その周辺のチャンクを自動的にロードします
				これによりクエリの精度が保証されます
				マップクエリ時にラグが発生する場合は、これを無効にしてください
				'''
		}

		LoadNPCHomeChunks: {
			Label: NPCホームチャンクをロード
			Tooltip:
				'''
				NPCが住むチャンクを常にロード状態に保ちます
				これがないと、NPCバナーが正しい位置に表示されない場合があります
				'''
		}
	}
