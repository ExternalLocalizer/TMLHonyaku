		BannerScale: {
			Label: バナーサイズ
			Tooltip:
				'''
				住宅バナーの描画サイズの倍率を調整します
				これによりバナーが従来のマップアイコンのように振る舞います
				'''
		}

		BannerTiles: {
			Label: バナータイル
			Tooltip:
				'''
				バナーが占めるマップタイルの数を調整します
				これによりバナーはバニラ風に見えますが、遠くにズームアウトすると見づらくなります
				'''
		}

		HoverScale: {
			Label: ホバーサイズ
			Tooltip: 住宅バナーにカーソルを合わせたときの描画サイズの倍率を調整します
		}

		DisplayOptions: {
			Label: 表示オプション
			Tooltip: ""
		}

		AllowHoverText: {
			Label: ホバーテキストを許可
			Tooltip: 住宅バナーにカーソルを合わせたときにホバーテキストを表示します
		}

		AllowClickActions: {
			Label: クリック操作を許可
			Tooltip: 住宅バナーをクリックできます
		}

		Enable: {
			Label: 有効化
			Tooltip: このマップタイプでバナーを表示します
		}
	}

	BannerScaleOptions: {
		Tooltip:
			'''
			サイズ値を使用：バナーの描画サイズを「バナーサイズ」で設定
			タイル値を使用：バナーの描画サイズを「バナータイル」で設定
			'''

		UseScaleValues: {
			Label: サイズ値を使用
		}

		UseTileValues: {
			Label: タイル値を使用
		}
	}

	BannerDisplayOptions: {
		Tooltip:
			'''
			バニラ：住宅パネルが開いている場合にバナーを表示します
			常に表示：バナーを常に表示します
			表示しない：バナーを表示しない
			'''

		Vanilla: {
			Label: バニラ
		}

		AlwaysShow: {
			Label: 常に表示
		}

		NeverShow: {
			Label: 表示しない
		}
	}

	HousingIconConfig: {
		Tooltip: ""

		DisplayOption: {
			Label: 表示オプション
			Tooltip:
				'''
				常に表示：住宅アイコンを常に表示します
				表示しない：住宅アイコンを表示しない
				'''
		}

		PositionOption: {
			Label: 位置オプション
			Tooltip:
				'''
				バニラ：住宅アイコンはインベントリで開いたときと同じ位置に表示します
				カスタム：住宅アイコンは以下のX、Y位置に基づいて配置します
				'''
		}

		HousingIconX: {
			Label: 住宅アイコンX
			Tooltip:
				'''
				住宅アイコンの水平位置を画面幅の割合で設定（0＝左、1＝右）
				右クリックでドラッグして住宅アイコンを移動できます
				'''
		}

		HousingIconY: {
			Label: 住宅アイコンY
			Tooltip:
				'''
				住宅アイコンの垂直位置を画面高さの割合で設定（0＝上、1＝下）
				右クリックでドラッグして住宅アイコンを移動できます
				'''
		}

		LockPosition: {
			Label: 位置を固定
			Tooltip: オフにすることでパネルを移動できます
		}

		Scale: {
			Label: サイズ
			Tooltip:
				'''
				住宅アイコンの大きさの倍率。
				デフォルト：1
				'''
		}

		AllowHoverText: {
			Label: ホバーテキストを許可
			Tooltip: 住宅アイコンにカーソルを合わせたときにホバーテキストを表示する
		}
	}

	IconDisplayOptions: {
		Tooltip: ""

		AlwaysShow: {
			Label: 常に表示
		}

		NeverShow: {
			Label: 表示しない
		}
